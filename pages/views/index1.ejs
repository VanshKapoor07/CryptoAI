    ];
      bookings.push(...serverEvents);

    const cardContainer = document.getElementById("card-container");
    const buttons = document.querySelectorAll("button");

    function renderCards(category) {
        cardContainer.innerHTML = ""; // Clear previous content
        const filteredBookings = category === "All" ? bookings : bookings.filter(booking => booking.category === category);

        filteredBookings.forEach(booking => {
            const card = document.createElement("div");
            card.className = "card bg-gray-800 text-white shadow-md";
            card.style.width = "18rem";
            card.innerHTML = `
                <img src="${booking.image}" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title">${booking.title}</h5>
                    <p class="card-text">Artist: ${booking.Artist}</p>
                    <p class="card-text">DATE: ${booking.date}</p>
                    <p class="card-text">${booking.description}</p>
                    <p class="card-text">VENUE: ${booking.Venue}</p>
                    <p class="card-text">TIME: ${booking.Time}</p>
                    <p class="card-text">Category: ${booking.category}</p>
                    <br>
                    <form method="post" action="/purchase_ticket">
                    <button class="btn primary-btn btn-lg" type="submit"> Purchase Now</button>
                </form>
                </div>
            `;
            cardContainer.appendChild(card);
        });
    }

    buttons.forEach(button => {
        button.addEventListener("click", () => {
            const category = button.textContent.replace(/\s+/g, "-"); // Convert text to category format
            renderCards(category === "All" ? "All" : category);
        });
    });

    renderCards("all"); // Default render all events on page load
</script>


